{
  "name": "the-end-page",
  "version": "1.0.0",
  "description": "A platform for users to share life events and track emotional journeys",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "build": "pkg .",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "bin": "src/index.js",
  "pkg": {
    "targets": ["node18-macos-x64"],
    "outputPath": "dist",
    "assets": [
      "src/**/*",
      "public/**/*",
      "uploads/**/*",
      ".env"
    ]
  },
  "keywords": [
    "journal",
    "emotions",
    "life events",
    "mood tracking"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "connect-flash": "^0.1.1",
    "dotenv": "^16.3.1",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-session": "^1.18.0",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.6.5",
    "natural": "^6.10.4",
    "nodemailer": "^6.9.7",
    "sequelize": "^6.35.2"
  },
  "devDependencies": {
    "eslint": "^8.56.0",
    "nodemon": "^3.0.2",
    "pkg": "^5.8.1"
